.log-list {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding: 0;
}
.log-item {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding:20rpx;
  width:100%;
  box-sizing:border-box;
}
.log-item:not(:last-child) {
  border-bottom: 1px solid #eee;
}
.hc {
  font-size:12px;
  color:#666;
}